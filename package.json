{
  "name": "Guitzli",
  "productName": "Guitzli",
  "version": "0.3.1",
  "description": "Convert image files for web using google's guetzli algorithm",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "make": "npm run make64 & npm run make32",
    "make64": "electron-forge make --arch=x64",
    "make32": "electron-forge make --arch=ia32"
  },
  "keywords": [
    "guetzli",
    "gui",
    "jpg",
    "compression",
    "conversion"
  ],
  "author": "Erik-Jan van Kesteren",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/vankesteren/guitzli.git"
  },
  "dependencies": {
    "electron-compile": "^6.3.0",
    "temp": "^0.8.3",
    "path": "^0.12.7",
    "url": "^0.11.0",
    "ps-tree": "^1.1.0",
    "image-size": "^0.5.4",
    "electron-squirrel-startup": "^1.0.0"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "dmg"
        ],
        "linux": [
          "deb"
        ]
      },
      "electronPackagerConfig": {
        "name": "Guitzli",
        "electronVersion": "1.6.2",
        "asar": {
          "unpackDir": "src/bin"
        },
        "icon": "build/icon.ico",
        "win32metadata": {
          "ProductName": "Guitzli",
          "CompanyName": "Erik-Jan van Kesteren"
        }
      },
      "electronWinstallerConfig": {
        "name": "Guitzli-setup",
        "iconUrl": "https://raw.githubusercontent.com/vankesteren/guitzli/master/build/icon.ico"
      },
      "electronInstallerDMG": {
        "background": "build/background.png",
        "icon": "build/icon.icns",
        "format": "ULFO"
      },
      "electronInstallerDebian": {
        "icon": "build/icon.png",
        "categories": [
          "Utility"
        ]
      },
      "github_repository": {
        "owner": "vankesteren",
        "name": "guitzli"
      },
      "windowsStoreConfig": {
        "packageName": "Guitzli"
      },
      "publish_targets": {
        "win32": [
          "github"
        ],
        "darwin": [
          "github"
        ],
        "linux": [
          "github"
        ]
      }
    }
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.3.3",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^3.0.3",
    "electron-prebuilt-compile": "1.6.2"
  }
}
